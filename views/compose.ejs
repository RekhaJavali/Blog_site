
<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for = "titleText">Title</label>
        <input type="text" id ="titletext" name="titleText" class="form-control" autofocus ="false">
    </div>
    <div class="form-group">
        <label for = "postText">Post</label>
        <textarea id ="postText" name="postBody" class="form-control" rows="4" autofocus ="false"></textarea>
    </div>
    <button type = "submit" name="btnvalue" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer"); -%>